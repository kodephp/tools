{
    "name": "kode/tools",
    "description": "PHP8.3+通用工具包，包含数组处理、字符串处理、时间处理、加解密等功能",
    "type": "library",
    "license": "MIT",
    "version": "1.1.3",
    "keywords": ["php", "array", "string", "time", "message", "crypto", "tree", "tools"],
    "homepage": "https://github.com/kode/tools",
    "authors": [
        {
            "name": "Kode Team",
            "email": "team@kode.com"
        }
    ],
    "support": {
        "issues": "https://github.com/kode/tools/issues",
        "docs": "https://github.com/kode/tools/blob/master/README.md"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/kode"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.20"
    },
    "autoload": {
        "psr-4": {
            "Kode\\": "src/"
        },
        "files": [
            "src/Helper/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kode\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse",
        "fix": "php-cs-fixer fix"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}